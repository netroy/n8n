{
  "name": "n8n-monorepo",
  "version": "0.186.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://n8n.io",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "yarn nx run-many --all --target=build",
    "dev": "yarn nx run-many --all --target=dev --parallel 10",
    "clean": "yarn nx run-many --all --target=clean",
    "format": "yarn nx run-many --all --target=format",
    "lint": "yarn nx run-many --all --target=lint",
    "lintfix": "yarn nx run-many --all --target=lintfix",
    "optimize-svg": "find ./packages -name '*.svg' ! -name 'pipedrive.svg' -print0 | xargs -0 -P16 -L20 npx svgo",
    "start": "run-script-os",
    "start:default": "cd packages/cli/bin && ./n8n",
    "start:windows": "cd packages/cli/bin && n8n",
    "test": "yarn nx run-many --all --target=test",
    "watch": "yarn nx run-many --all --target=watch",
    "webhook": "./packages/cli/bin/n8n webhook",
    "worker": "./packages/cli/bin/n8n worker"
  },
  "devDependencies": {
    "nx": "14.4.3",
    "run-script-os": "^1.0.7"
  },
  "resolutions": {
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.22",
    "express": "4.18.1",
    "eslint": "7.32.0",
    "jest": "27.5.1",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "ts-node": "8.9.1",
    "tslint": "6.1.2",
    "typescript": "4.6.4"
  },
  "postcss": {}
}
