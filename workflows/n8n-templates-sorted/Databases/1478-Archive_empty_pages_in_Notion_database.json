            [
              {
                "node": "Get Page Blocks",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "Every day @ 2am": {
          "main": [
            [
              {
                "node": "Get All Databases",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "Get Page Blocks": {
          "main": [
            [
              {
                "node": "Process Blocks",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "Get All Databases": {
          "main": [
            [
              {
                "node": "Get All Database Pages",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "If Empty Properties": {
          "main": [
            [
              {
                "node": "SplitInBatches",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "Get All Database Pages": {
          "main": [
            [
              {
                "node": "Check for empty properties",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "Check for empty properties": {
          "main": [
            [
              {
                "node": "If Empty Properties",
                "type": "main",
                "index": 0
              }
            ]
          ]
        }
      }
