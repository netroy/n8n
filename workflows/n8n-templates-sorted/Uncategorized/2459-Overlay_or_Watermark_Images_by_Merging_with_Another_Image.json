        {
          "id": "29e77ce2-15a0-47a8-8b1c-8f457ae435c6",
          "name": "Nest Bg Meta",
          "type": "n8n-nodes-base.set",
          "position": [
            1480,
            60
          ],
          "parameters": {
            "options": {
              "includeBinary": true
            },
            "assignments": {
