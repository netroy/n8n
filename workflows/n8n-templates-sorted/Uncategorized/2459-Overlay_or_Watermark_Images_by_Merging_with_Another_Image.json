          ],
          "parameters": {
            "mode": "runOnceForEachItem",
            "jsCode": "$input.item.binary.top = $input.item.binary.data;\ndelete $input.item.binary.data;\nreturn $input.item;"
          },
          "typeVersion": 2
        },
        {
          "id": "90b0e990-d330-4875-b492-28d52019784d",
