          "name": "Find tasks",
          "type": "n8n-nodes-base.notion",
          "position": [
            980,
            160
          ],
          "parameters": {
            "options": {},
            "resource": "databasePage",
            "operation": "getAll",
            "databaseId": "6181df20-c949-42e3-9999-7168d746efab",
            "filterJson": "={{$node[\"Get unique tasks\"].json[\"notionfilter\"]}}",
