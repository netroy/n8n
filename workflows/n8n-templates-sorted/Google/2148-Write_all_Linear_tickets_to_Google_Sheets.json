            "variables": "={\n  \"filter\": {\n    \"team\": {\n      \"name\":  {\n        \"eq\": \"Adore\"\n      }\n    }\n  }\n}",
            "requestFormat": "json",
            "authentication": "headerAuth"
          },
          "credentials": {
            "httpHeaderAuth": {
              "id": "QNQL42DMISvQl50S",
              "name": "Linear api "
