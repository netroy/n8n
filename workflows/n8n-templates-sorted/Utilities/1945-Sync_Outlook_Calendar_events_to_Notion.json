                }
              ]
            }
          },
          "credentials": {
            "notionApi": {
              "id": "18",
              "name": "[UPDATE ME]"
            }
          },
          "typeVersion": 2
        },
        {
          "id": "ee4792c4-d71c-4fd3-a8a3-babae5ff3479",
          "name": "X days into the future",
          "type": "n8n-nodes-base.dateTime",
          "position": [
            1500,
            840
          ],
          "parameters": {
            "duration": 365,
            "magnitude": "={{ $json.timestamp }}",
