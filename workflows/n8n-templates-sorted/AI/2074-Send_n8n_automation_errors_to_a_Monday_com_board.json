          "notes": "CREATE ERROR ITEM",
          "position": [
            620,
            240
          ],
          "parameters": {
            "name": "={{ \"\".concat($('Error Trigger').last().json.execution.id) }}",
            "boardId": "1382091189",
            "groupId": "topics",
            "resource": "boardItem",
            "additionalFields": {}
          },
          "credentials": {
            "mondayComApi": {
              "id": "SP53wbPUCBNJRq1G",
