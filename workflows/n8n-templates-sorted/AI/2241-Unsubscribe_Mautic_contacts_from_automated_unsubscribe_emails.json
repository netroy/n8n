            }
          },
          "typeVersion": 1
        },
        {
          "id": "b26ddbb9-3209-458b-8e94-2854ed8bf8de",
          "name": "Reply Unsubscribe Message",
          "type": "n8n-nodes-base.gmail",
          "position": [
            1520,
            1140
          ],
          "parameters": {
            "message": "={{$node[\"Edit Fields\"].json[\"unsubscribeMessage\"]}}",
            "options": {},
            "messageId": "={{ $node[\"Gmail Trigger\"].json[\"id\"] }}",
