        {
          "id": "34fc931b-f692-4383-a75b-76502c11452b",
          "name": "Add to Do Not Contact List",
          "type": "n8n-nodes-base.mautic",
          "disabled": true,
          "position": [
            1520,
            520
          ],
          "parameters": {
            "contactId": "{{ $json[\"id\"] }}",
            "operation": "editDoNotContactList",
            "authentication": "oAuth2",
            "additionalFields": {}
          },
