          "position": [
            1520,
            920
          ],
          "parameters": {
            "resource": "contactSegment",
            "contactId": "={{ $json[\"id\"] }}",
            "operation": "remove",
            "segmentId": 1,
            "authentication": "oAuth2"
          },
          "credentials": {
