      "meta": {
        "instanceId": "08daa2aa5b6032ff63690600b74f68f5b0f34a3b100102e019b35c4419168977",
        "templateCredsSetupCompleted": true
      },
      "name": "Get Long Lived Facebook User or Page Access Token",
      "tags": [],
      "nodes": [
