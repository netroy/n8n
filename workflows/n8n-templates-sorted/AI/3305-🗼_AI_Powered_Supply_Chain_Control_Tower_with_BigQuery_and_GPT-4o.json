      "nodes": [
        {
          "id": "53b36910-966f-45ba-a425-a3260a55059f",
          "name": "OpenAI Chat Model",
          "type": "@n8n/n8n-nodes-langchain.lmChatOpenAi",
          "position": [
            340,
            480
          ],
          "parameters": {
            "model": {
              "__rl": true,
