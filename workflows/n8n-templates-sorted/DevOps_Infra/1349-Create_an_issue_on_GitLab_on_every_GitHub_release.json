              {
                "node": "Merge",
                "type": "main",
                "index": 0
              }
            ]
          ]
        },
        "No release for issue?": {
          "main": [
            [
              {
                "node": "Create issue",
                "type": "main",
                "index": 0
              }
            ]
          ]
        }
      }
