{
	"compilerOptions": {
		"types": ["node"],
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"declaration": true,

		"rootDir": "${configDir}/src",
		"tsBuildInfoFile": "${configDir}/dist/build.tsbuildinfo"
	},
	"tsc-alias": {
		"replacers": {
			"base-url": {
				"enabled": false
			}
		}
	}
}
